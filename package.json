{
  "name": "podverse-api",
  "version": "4.15.20",
  "description": "Data API, database migration scripts, and backend services for all Podverse models.",
  "contributors": [
    "Mitch Downey"
  ],
  "main": "dist/bundle.js",
  "scripts": {
    "lint:prettier": "npm run prettier:check",
    "prettier:base": "prettier",
    "prettier:list": "npm run prettier:base -- --list-different  \"src/**/*.{ts,js}\" \"__tests__/**/*.{ts,js}\"",
    "prettier:check": "npm run prettier:base -- --check  \"src/**/*.{ts,js}\" \"__tests__/**/*.{ts,js}\"",
    "prettier:write": "npm run prettier:base -- --write",
    "prettier:fix": "npm run prettier:write --  \"src/**/*.{ts,js}\" \"__tests__/**/*.{ts,js}\"",
    "build": "webpack --progress --colors",
    "dev": "NODE_ENV=development ts-node -r dotenv/config -r tsconfig-paths/register src/server.ts",
    "dev:watch": "NODE_ENV=development nodemon --watch 'src' --watch $(realpath node_modules/podverse-external-services) --watch $(realpath node_modules/podverse-orm) --watch $(realpath node_modules/podverse-parser) --watch $(realpath node_modules/podverse-shared) -e js,ts,tsx,json --exec ts-node -r dotenv/config -r tsconfig-paths/register src/server.ts",
    "dev:seeds:categories": "ts-node -r dotenv/config -r tsconfig-paths/register ./src/seeds/categories.ts",
    "dev:seeds:podcasts": "ts-node -r dotenv/config -r tsconfig-paths/register ./src/seeds/podcasts.ts",
    "dev:seeds:qa:populateDatabase": "ts-node -r dotenv/config -r tsconfig-paths/register ./src/seeds/qa/populateDatabase.ts",
    "lint:ts": "eslint src/**/*.ts",
    "seeds:categories": "ts-node -r tsconfig-paths/register ./src/seeds/categories.ts",
    "seeds:podcasts": "ts-node -r tsconfig-paths/register ./src/seeds/podcasts.ts",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "start:local": "ts-node -r tsconfig-paths/register src/server.local.ts",
    "test:unit": "jest",
    "postinstall": "patch-package"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "npm run prettier:fix",
      "eslint .",
      "git add"
    ]
  },
  "license": "AGPLv3",
  "homepage": "https://github.com/podverse/podverse-api#readme",
  "keywords": [
    "podcast",
    "episode",
    "RSS",
    "Atom",
    "feed",
    "parser",
    "feedparser",
    "database",
    "db",
    "sequelize",
    "podverse"
  ],
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "3.0.0",
    "@types/crypto-js": "^3.1.47",
    "@types/dotenv": "8.2.0",
    "@types/google.analytics": "0.0.40",
    "@types/jest": "26.0.3",
    "@types/koa": "2.11.0",
    "@types/koa-bodyparser": "5.0.1",
    "@types/koa-helmet": "3.1.2",
    "@types/koa-jwt": "3.3.0",
    "@types/koa-passport": "4.0.2",
    "@types/koa-router": "7.0.42",
    "@types/lodash": "4.14.200",
    "@types/koa__cors": "2.2.3",
    "@types/node": "12.12.14",
    "@types/passport": "1.0.2",
    "@types/passport-jwt": "3.0.3",
    "@types/passport-local": "1.0.33",
    "@types/shelljs": "0.8.6",
    "@types/validator": "12.0.1",
    "@types/web-push": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.54.0",
    "jest": "26.1.0",
    "koa2-swagger-ui": "2.15.4",
    "lint-staged": "9.5.0",
    "nodemon": "2.0.1",
    "patch-package": "6.5.0",
    "prettier": "^2.5.1",
    "ts-node": "10.9.2",
    "typescript": "^5.3.3",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10"
  },
  "dependencies": {
    "@koa/cors": "3.0.0",
    "archiver": "^5.3.1",
    "axios": "1.6.0",
    "bcryptjs": "2.4.3",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "class-validator": "0.14.0",
    "clean-webpack-plugin": "3.0.0",
    "cookie": "0.4.0",
    "crypto-js": "~4.2.0",
    "date-fns": "2.8.1",
    "googleapis": "45.0.0",
    "http-errors": "1.7.3",
    "husky": "3.1.0",
    "isomorphic-fetch": "^3.0.0",
    "joi": "14.3.1",
    "koa": "2.11.0",
    "koa-bodyparser": "4.2.1",
    "koa-helmet": "5.2.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "3.6.0",
    "koa-mount": "4.0.0",
    "koa-passport": "4.1.3",
    "koa-router": "7.4.0",
    "koa-session": "5.12.3",
    "koa-static": "5.0.0",
    "koa2-ratelimit": "0.9.1",
    "lodash": "4.17.21",
    "moment": "^2.29.4",
    "nodemailer": "6.6.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "password-validator": "5.0.3",
    "paypal-rest-sdk": "2.0.0-rc.2",
    "podcast-partytime": "4.6.2",
    "podverse-external-services": "^1.0.0",
    "podverse-orm": "^1.0.0",
    "podverse-parser": "^1.0.0",
    "podverse-shared": "^4.15.3",
    "reflect-metadata": "0.1.13",
    "request": "^2.88.2",
    "request-promise-native": "1.0.8",
    "shortid": "2.2.15",
    "sqlstring": "2.3.3",
    "threadcap": "0.1.8",
    "ts-loader": "6.2.1",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "uuid": "^9.0.1",
    "valid-url": "1.0.9",
    "validator": "13.7.0",
    "web-push": "^3.6.3",
    "winston": "3.2.1",
    "ws": "^8.5.0"
  }
}
